{
  "name": "3c-portfolio-manager",
  "productName": "3C Portfolio Manager",
  "version": "3.0.0",
  "description": "An application to manage your 3Commas portfolio.",
  "private": true,
  "main": "./dist/main.js",
  "scripts": {
    "check-lint": "eslint --ext .js,.jsx,.tsx,.ts . --quiet --cache",
    "fix-lint": "eslint --ext .js,.jsx,.tsx,.ts . --quiet --cache --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
    "pack": "electron-builder --dir ",
    "build:mac": "npm run production && env DEBUG=electron-builder && electron-builder --dir && electron-builder --mac",
    "build:win": "npm run production && electron-builder --dir && electron-builder --win",
    "build:linux": "npm run production && electron-builder --dir && electron-builder --linux",
    "build": "npm run production && electron-builder --dir && electron-builder -mwl",
    "react:dev": "webpack serve --config webpack.dev.js --stats-error-details",
    "electron:dev": "webpack --config ./webpack.electron.js && electron .",
    "production": "webpack  --config webpack.prod.js",
    "dev": "concurrently --kill-others \"npm run react:dev\" \"npm run electron:dev\"",
    "start": "npm run build && electron dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/3cpm/desktop"
  },
  "author": "Colton",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/3cpm/desktop/issues"
  },
  "homepage": "https://github.com/3cpm/desktop",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@electron/typescript-definitions": "^8.9.5",
    "@types/better-sqlite3": "^7.5.0",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^11.15.54",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-table": "^7.7.10",
    "@types/recharts": "^1.8.23",
    "@types/styled-components": "^5.1.12",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "d3-scale": "^3.3.0",
    "d3-scale-chromatic": "^2.0.0",
    "electron": "^18.0.1",
    "electron-builder": "^23.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-notarize": "^1.2.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.5.1",
    "msw": "^0.39.2",
    "react-gauge-chart": "^0.4.0",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.1.0",
    "styled-components": "^5.3.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.4",
    "typescript": "^4.6.2",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.5.1",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.5.3",
    "@mui/styles": "^5.5.3",
    "@reduxjs/toolkit": "^1.8.1",
    "better-sqlite3": "^7.5.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.22.1",
    "date-fns-tz": "^1.1.6",
    "dot-prop": "^7.2.0",
    "electron-context-menu": "^3.1.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.4.6",
    "electron-store": "^8.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "fs-extra": "^10.0.0",
    "material-ui-popup-state": "^2.0.0",
    "moment": "^2.29.1",
    "node-fetch": "^3.2.3",
    "react": "^18.0.0",
    "react-d3-speedometer": "^1.0.1",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.0-beta.3",
    "react-router-dom": "^6.3.0",
    "react-table": "^7.7.0",
    "recharts": "^2.1.9",
    "uuid": "^8.3.2"
  },
  "build": {
    "appId": "com.savvytoolbelt.3cportfoliomanager",
    "artifactName": "${name}-${version}-${os}-${arch}.${ext}",
    "afterSign": "scripts/notarize.js",
    "directories": {
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.finance",
      "target": [
        "dmg",
        "zip"
      ],
      "type": "distribution",
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "gatekeeperAssess": false,
      "icon": "assets/icons/icons.icns",
      "provisioningProfile": "build/MacAppStore.provisionprofile",
      "extraFiles": [
        "build/MacAppStore.provisionprofile"
      ]
    },
    "mas": {
      "entitlements": "build/entitlements.mas.plist",
      "entitlementsInherit": "build/entitlements.mas.inherit.plist",
      "provisioningProfile": "build/MacAppStore.provisionprofile",
      "hardenedRuntime": false,
      "icon": "assets/icons/icons.icns"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "icon": "assets/icons/icons.icns"
    },
    "win": {
      "icon": "assets/icons/icon.ico",
      "publisherName": "Colton Shaw",
      "target": "NSIS"
    },
    "nsis": {
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "linux": {
      "icon": "assets/icons"
    }
  }
}
